{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://restcountries.com",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@testing-library/dom": "^10.4.0",
    "axios": "^1.8.4",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test-CountryFlags": "react-scripts test src/components/pages/CountryFlags.test.js --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test-CountryPages": "react-scripts test src/components/pages/CountryPage.test.js --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test-CountryDetails": "react-scripts test src/components/common/CountryDetails.test.js --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test-HomePage": "react-scripts test src/components/pages/HomePage.test.js --transformIgnorePatterns \"node_modules/(?!axios)/\""
